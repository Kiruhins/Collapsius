<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back__xz"
    tools:context=".configuration_editable">

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="30dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/animescape1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/imageButton8" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="30dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/time"
        app:layout_constraintEnd_toStartOf="@+id/textView15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton4" />

    <TextView
        android:id="@+id/Textwiew9"
        android:layout_width="260dp"
        android:layout_height="44dp"
        android:background="@drawable/settings"
        android:fontFamily="@font/alata"
        android:text="Количество игроков"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.082" />

    <ImageButton
        android:id="@+id/bt_player2_2"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/pusto"
        app:layout_constraintBottom_toBottomOf="@+id/bt_player3_3"
        app:layout_constraintEnd_toStartOf="@+id/bt_player3_3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/bt_player3_3" />

    <ImageButton
        android:id="@+id/bt_backks"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/pusto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView15"
        app:layout_constraintHorizontal_bias="0.7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton9" />

    <ImageButton
        android:id="@+id/bt_nextts"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/pusto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.34"
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/imageButton10" />

    <ImageButton
        android:id="@+id/bt_player3_3"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/pusto"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Textwiew9" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/alata"
        android:text="3"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/bt_player3_3"
        app:layout_constraintEnd_toEndOf="@+id/bt_player3_3"
        app:layout_constraintStart_toStartOf="@+id/bt_player3_3"
        app:layout_constraintTop_toTopOf="@+id/bt_player3_3" />

    <ImageButton
        android:id="@+id/bt_player4_4"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/pusto"
        app:layout_constraintBottom_toBottomOf="@+id/bt_player3_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.341"
        app:layout_constraintStart_toEndOf="@+id/bt_player3_3"
        app:layout_constraintTop_toTopOf="@+id/bt_player3_3"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/alata"
        android:text="4"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/bt_player4_4"
        app:layout_constraintEnd_toEndOf="@+id/bt_player4_4"
        app:layout_constraintStart_toStartOf="@+id/bt_player4_4"
        app:layout_constraintTop_toTopOf="@+id/bt_player4_4" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="250dp"
        android:layout_height="44dp"
        android:background="@drawable/settings"
        android:fontFamily="@font/alata"
        android:text="Выберите настройки"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.383" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/alata"
        android:text="@string/_2"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/bt_player2_2"
        app:layout_constraintEnd_toEndOf="@+id/bt_player2_2"
        app:layout_constraintStart_toStartOf="@+id/bt_player2_2"
        app:layout_constraintTop_toTopOf="@+id/bt_player2_2" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/animportal1"
        app:layout_constraintEnd_toStartOf="@+id/textView15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView15" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/animwall1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toTopOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="31dp"
        android:layout_height="408dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintVertical_bias="0.547" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/alata"
        android:text="Назад"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/bt_backks"
        app:layout_constraintEnd_toEndOf="@+id/bt_backks"
        app:layout_constraintStart_toStartOf="@+id/bt_backks"
        app:layout_constraintTop_toTopOf="@+id/bt_backks" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/alata"
        android:text="Далее"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/bt_nextts"
        app:layout_constraintEnd_toEndOf="@+id/bt_nextts"
        app:layout_constraintStart_toStartOf="@+id/bt_nextts"
        app:layout_constraintTop_toTopOf="@+id/bt_nextts" />

</androidx.constraintlayout.widget.ConstraintLayout>